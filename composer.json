{
  "name": "phpffcms/ffcms",
  "description": "FFCMS 3 version based on MVC core ffcms-core.",
  "keywords": [
    "php",
    "cms",
    "mvc",
    "framework",
    "php5",
    "pdo",
    "ffcms"
  ],
  "type": "project",
  "homepage": "https://ffcms.org",
  "license": "MIT",
  "minimum-stability": "beta",
  "config": {
    "process-timeout": 1800
  },
  "require": {
    "php": ">=5.5",
    "lib-curl": "*",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-zip": "*",
    "phpffcms/ffcms-core": "1.0.*",
    "phpffcms/ffcms-console": "1.0.*",
    "phpffcms/ffcms-ckeditor": "1.*@stable",
    "phpffcms/searchable": "1.*@stable",
    "phpffcms/language-flags": "1.*@stable",
    "phpffcms/ffcms-elfinder": "1.*@stable",
    "illuminate/database": "5.*@stable",
    "symfony/http-foundation": "2.8@stable",
    "maximebf/debugbar": "1.*@stable",
    "ezyang/htmlpurifier": "4.*@stable",
    "gregwar/image": "2.*@stable",
    "dflydev/dot-access-data": "1.0.*@stable",
    "swiftmailer/swiftmailer": "5.4.*@stable",
    "gregwar/captcha": "1.*@stable",
    "phpfastcache/phpfastcache": "4.*@stable",
    "suin/php-rss-writer": "1.3.*@stable",
    "hybridauth/hybridauth": "2.6.*@stable",
    "bower-asset/jquery": "2.1.*@stable | 1.11.*@stable",
    "bower-asset/jquery-validation": "1.*@stable",
    "bower-asset/bootstrap": "3.3.* | 3.2.* | 3.1.*",
    "bower-asset/jquery-ui": "1.11.*@stable",
    "bower-asset/components-font-awesome": "4.*@stable",
    "bower-asset/dropzone": "4.3.*@stable"
  },
  "authors": [
    {
      "name": "Mihail Pyatinskyi",
      "email": "zenn1989@gmail.com",
      "homepage": "http://ffcms.ru",
      "role": "Developer"
    }
  ],
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
